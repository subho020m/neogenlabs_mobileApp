<?xml version="1.0" encoding="UTF-8"?><components xmlns:scr="http://www.osgi.org/xmlns/scr/v1.0.0">
    <scr:component name="sling.sling.servlet.ByResourceTypeServlet">
        <implementation class="sling.sling.servlet.ByResourceTypeServlet"/>
        <service servicefactory="false">
            <provide interface="javax.servlet.Servlet"/>
        </service>
        <property name="sling.servlet.resourceTypes">
            sling/servlet/default
        </property>
        <property name="sling.servlet.selectors">
            hello
        </property>
        <property name="sling.servlet.extensions">
            html
        </property>
        <property name="service.description" value="Hello World Type Servlet"/>
        <property name="service.vendor" value="The Apache Software Foundation"/>
        <property name="service.pid" value="sling.sling.servlet.ByResourceTypeServlet"/>
    </scr:component>
</components>
